{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint", "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended", "prettier"
    ],
    "rules": {
        "no-console": 1, // this means warn!
        "prettier/prettier": ["error"]
    }
  }